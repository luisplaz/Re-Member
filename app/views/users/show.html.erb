<h1 class="profile-title"> Welcome, <%= current_user.username %> </h1>

<div class="profile-nav">
	<div class="heading"> My Decks </div>
	<div class="deck-add-button">
		<%= link_to 'Create Deck',new_deck_path %>
	</div>
</div>

<% @user.decks.each do |deck| %>
<h4><%= link_to deck.name, deck_path(deck.id) %></h4>
<% end %>